
import java.util.Scanner;


public class Test{


 void add (int a, int b)
   {
       System.out.println(a+b);
   }
   void add (double a, double b)
   {
       System.out.println(a+b);
   }

   void add (int a, int b, int c)
   {
       System.out.println(a+b+c);
   }
    public static void main(String[] args) {

        Test ob = new Test();
        ob.add(5,10);
        ob.add(7.5,5.6);
        ob.add(15,15,12);
    }

}
